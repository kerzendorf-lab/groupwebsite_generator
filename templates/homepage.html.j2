    <!-- Projects Section -->
    <section class="about-section py-5">
        <div class="container py-lg-5 py-md-4 py-2">
            <div class="title-heading-w3 mx-auto text-center mb-sm-5 mb-4 pb-xl-4">
                <h3 class="w3l-title-main">Recent Research</h3>
            </div>
            <div class="row justify-content-center">
                {% for content in recent_content[:3] %}
                {% set content_data = ContentData(content['article_id']) %}
                <div class="col-lg-4 col-md-6">
                    <div class="about-single" style="width: 374px; height: 512px;">
                        <div class="about-icon mb-4">
                        <a href="sub_research/{{ content_data.load_json().category|replace(' ', '_')|lower }}/{{ page_link(content_data.load_json().article_id)|lower }}.html">
                            <img style="max-width: 294px; max-height: 224px;" src="{{ content_data.load_json().cover_image }}"></a>
                        </div>
                        <div class="about-content">
                            <h5 class="mb-3" style="height: 55px; overflow: hidden; display: flex; align-items: center;">
                                <a style="overflow: hidden; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 2; -webkit-box-orient: vertical;" href="sub_research/{{ content_data.load_json().category|replace(' ', '_')|lower }}/{{ page_link(content_data.load_json().article_id)|lower }}.html">
                                    {{ content_data.load_json().title }}
                                </a>
                            </h5>
                            <hr style="border-top: 1px dotted rgba(143, 143, 143);">
                          
                                <p class="mb-3" style="overflow: hidden; height: 80px; overflow: hidden; align-items: center; text-overflow: ellipsis; display: -webkit-box; -webkit-line-clamp: 3; -webkit-box-orient: vertical;">
                                    {{ content_data.load_json().short_description }}
                                </p>
                         
                        </div>
                    </div>
                </div>
                {% endfor %}
            </div>
        </div>
    </section>
