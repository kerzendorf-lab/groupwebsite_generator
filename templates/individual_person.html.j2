    <!-- Include PDF.js library -->
    <script src="../../assets/js/pdfjs-3/build/pdf.js"></script>

    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const carousel = document.querySelector(".carousel");
            const slides = carousel.querySelectorAll(".slide");

            let currentSlide = 0;

            function showSlide(slideIndex) {
                // Hide all slides
                slides.forEach((slide) => {
                    slide.style.display = "none";
                });

                // Show the current slide
                slides[slideIndex].style.display = "block";

                // Render PDF if the slide contains an embed element
                const embedElement = slides[slideIndex].querySelector("embed");
                if (embedElement) {
                    const pdfUrl = embedElement.getAttribute("src");

                    // Use PDF.js to render the PDF
                    const canvas = document.createElement("canvas");
                    slides[slideIndex].appendChild(canvas);

                    PDFJS.getDocument(pdfUrl)
                        .then(function (pdf) {
                            return pdf.getPage(1);
                        })
                        .then(function (page) {
                            const viewport = page.getViewport({ scale: 1 });
                            const context = canvas.getContext("2d");
                            canvas.height = viewport.height;
                            canvas.width = viewport.width;

                            const renderContext = {
                                canvasContext: context,
                                viewport: viewport,
                            };

                            page.render(renderContext);
                        });
                }
            }

            function showNextSlide() {
                currentSlide = (currentSlide + 1) % slides.length;
                showSlide(currentSlide);
            }

            // Show the first slide
            showSlide(currentSlide);

            // Example: Attach click event to a next button
            const nextButton = document.querySelector("#nextButton");
            nextButton.addEventListener("click", showNextSlide);
        });
    </script>

